{{define "Layout"}}

<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="storiesboard hompage">
    <meta name="author" content="marcel schlimper">
    <!-- link rel="icon" type="image/png" href="/style/favicon-32x32.png" sizes="32x32" / -->

    <title>storiesboard</title>

    <!-- Bootstrap core CSS -->
    <link href="/style/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <!-- Custom styles for this page -->
    <link href="/style/sb.css" rel="stylesheet">

  </head>

  <body>
    <div class="blog-masthead">
      <div class="container">
        <nav class="nav">
          <a class="nav-link" href='/'> Home </a>
          <a class="nav-link" href='/'> About </a>
          <a class="nav-link" href='/'> Imprint </a>
        </nav>
      </div>
    </div>
    <div class="blog-header">
      <div class="container">
        <h1 class="blog-title">
          storiesboard blog
        </h1>
        <p class="lead blog-description">
          artificial intelligence for everyone.
        </p>
      </div>
    </div>
    <div class = "container">
      <div class= "row">
        <div class= "col-sm-8 blog-main">
          {{template "Content" .}}
        </div>
        <div class= "col-sm-3 offset-sm-1 blog-sidebar">

          <div class="sidebar-module sidebar-module-inset">
            <h4>About</h4>
            <p>this blog will have a look at different ai frameworks.</p>
          </div>

          <div class="sidebar-module">
            <h4>Archives</h4>
            <ol class="list-unstyled">
              <li><a href="#">September 2017</a></li>
            </ol>
          </div>

          <div class="sidebar-module">
            <h4>Elsewhere</h4>
            <ol class="list-unstyled">
              <li><a href="#">GitHub</a></li>
              <li><a href="#">Twitter</a></li>
              <li><a href="#">Facebook</a></li>
            </ol>
          </div>

        </div>
      </div>
    </div>



    <script src="/style/bootstrap/js/bootstrap.min.js"></script>
    <script>
      (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
        (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
        m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
      })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

      ga('create', 'UA-43809904-1', 'auto');
      ga('send', 'pageview');
    </script>
  </body>
</html>
{{end}}
